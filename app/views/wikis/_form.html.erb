<%= form_for @wiki do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title, class: 'form-control', placeholder: "Enter wiki title" %>

  <%= f.label :body %>
  <%= f.text_area :body, rows:12, class: 'form-control', placeholder: "Enter wiki body" %>

  <div class="form-group">
    <%= f.submit "Save", class: 'btn btn-success' %>
  </div>
<% end %>
